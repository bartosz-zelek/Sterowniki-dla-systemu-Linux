dml 1.2;

bank csr {
    /* --------------------------------------------------------------
       PCIe REGISTERS
       -------------------------------------------------------------- */

    register gcr          @ 0x05b00 "PCIe Control";
    register gscl_1       @ 0x05b10 "PCIe Statistics Control #1";
    register gscl_2       @ 0x05b14 "PCIe Statistics Control #2";
    register gscl_3       @ 0x05b18 "PCIe Statistics Control #3";
    register gscl_4       @ 0x05b1c "PCIe Statistics Control #4";
    register gscn_0       @ 0x05b20 "PCIe Counter #0";
    register gscn_1       @ 0x05b24 "PCIe Counter #1";
    register gscn_2       @ 0x05b28 "PCIe Counter #2";
    register gscn_3       @ 0x05b2c "PCIe Counter #3";
    register factps       @ 0x05b30 "Function Action and Power State to MNG";
    register gioanactl0   @ 0x05b34 "Serdes/CCM/PCIe CSR";
    register gioanactl1   @ 0x05b38 "Serdes/CCM/PCIe CSR";
    register gioanactl2   @ 0x05b3c "Serdes/CCM/PCIe CSR";
    register gioanactl3   @ 0x05b40 "Serdes/CCM/PCIe CSR";
    register gioanactlall @ 0x05b44 "Serdes/CCM/PCIe CSR";
    register ccmctl       @ 0x05b48 "Serdes/CCM/PCIe CSR";
    register scctl        @ 0x05b4c "Serdes/CCM/PCIe CSR";
    register swsm         @ 0x05b50 "Software Semaphore";
    register fwsm         @ 0x05b54 "Firmware Semaphore";
    register swsm2        @ 0x05b58 "Software Semaphore 2";

    register gcr is (unimplemented);
    register gscl_1 is (unimplemented);
    register gscl_2 is (unimplemented);
    register gscl_3 is (unimplemented);
    register gscl_4 is (unimplemented);
    register gscn_0 is (unimplemented);
    register gscn_1 is (unimplemented);
    register gscn_2 is (unimplemented);
    register gscn_3 is (unimplemented);
    register factps is (unimplemented);
    register gioanactl0 is (unimplemented);
    register gioanactl1 is (unimplemented);
    register gioanactl2 is (unimplemented);
    register gioanactl3 is (unimplemented);
    register gioanactlall is (unimplemented);
    register ccmctl is (unimplemented);
    register scctl is (unimplemented);
    register swsm is (unimplemented);
    register fwsm is (unimplemented);
    register swsm2 is (unimplemented);
}
