# PROJECT_SETUP_PYTHON_ARGUMENT

# ignore any user startup code that may not work without Python version
unset PYTHONSTARTUP

DIR=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )
. "${DIR}/../scripts/python-path.sh"
. "${DIR}/../scripts/simics-python.sh"
exec "${_MINI_PYTHON}" "$@"
